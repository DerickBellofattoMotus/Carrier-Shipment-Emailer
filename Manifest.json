{
  "name": "Carrier Shipment Emailer",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "webRequest",
    "downloads"
  ],
  "host_permissions": [
    "https://app.turvo.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  }
}